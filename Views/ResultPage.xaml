<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="slunaSIIITC2.Views.ResultPage"
             Title="ResultPage">
    <ContentPage.Content>
        <StackLayout Padding="20" BackgroundColor="{AppThemeBinding Light='LightGray', Dark='#1E1E1E'}">
            <Label x:Name="infoLabel" FontSize="16"/>
            <Button Text="Exportar a archivo" Clicked="OnExportClicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>